    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.Base">
        <!-- Customize your theme here. -->

    </style>
    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@android:color/white</item>
    </style>


    <style name="ActionBarText">
        <item name="android:textColor">@color/white_80</item>
        <item name="android:textSize">@dimen/text_size_15</item>
    </style>



    <!--导航页  底部布局样式 -->
    <style name="Navgation_layout">
        <item name="android:layout_width">@dimen/dp_10</item>
        <item name="android:paddingTop">@dimen/dp_6</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:paddingBottom">@dimen/dp_5</item>
    </style>

    <!--导航页  底部image样式 -->
    <style name="Navgation_image">
        <item name="android:paddingTop">@dimen/dp_22</item>
        <item name="android:layout_width">@dimen/dp_20</item>
        <item name="android:layout_height">@dimen/dp_6</item>
        <item name="android:paddingBottom">@dimen/dp_14</item>

    </style>




